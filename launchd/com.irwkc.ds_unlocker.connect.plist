<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.irwkc.ds_unlocker.connect</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>-lc</string>
		<string>"$HOME/Desktop/Ds_unlocker/scripts/ctl.sh" connect</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardOutPath</key>
	<string>$HOME/Library/Logs/ds_unlocker.out.log</string>
	<key>StandardErrorPath</key>
	<string>$HOME/Library/Logs/ds_unlocker.err.log</string>
</dict>
</plist>
